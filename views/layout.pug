doctype html
html
  head
    title= title
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous")
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(src="https://kit.fontawesome.com/4c8d3e9977.js" crossorigin="anonymous")
  body
    block content

    div.header_div
      nav(class="nav nav-pills nav-fill fixed-top" style="padding: 5px 5px 5px 0px; margin:0; background: #333;")
        img.headerImg(src="images/mspb_logo.jpeg" alt="MSPB" width="50" height="50")
        a(class="btn bProp" data-bs-toggle="offcanvas" href="#offcanvasExample" role="button" aria-controls="offcanvasExample")
          span(class="fa-solid fa-bars fa-2xl fa-beat-fade lProp")
        div.aDiv
          a(class="nav-link aProp" href="/")  Home
          a(class="nav-link aProp" aria-current="page" href="#") Provider
          a(class="nav-link aProp" href="#") Manager
          a(class="nav-link aProp" href="#") Staff
          a(class="nav-link aProp" href="/faqs") FAQs
      div.offCanvasStyle(class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasExample" aria-labelledby="offcanvasExampleLabel")
        div(class="offcanvas-header")
          h3(style="font-size: 5em;") Menu
          button(type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close" style="margin-right: 10px;")
        div(class="offcanvas-body" style="padding: 0; margin: 0")
          div(class="dropdown mt-2" style="padding: 0; margin: 0")
            ul(aria-labelledby="dropdownMenuButton" style="padding: 0; margin: 0")
              li.ddoption Provider Tutorials
              li.ddoption Office Manager Tutorials
              li.ddoption Office Staff Tutorials
              li.ddoption 
                a(style="text-decoration:none" href="/faqs") FAQs

    script(src='/javascripts/getQuestionText.js')

    script(src='/javascripts/faqs.js')
    script(src='/javascripts/sendDMail.js')
    script(src='/javascripts/jquery.min.js')
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous")